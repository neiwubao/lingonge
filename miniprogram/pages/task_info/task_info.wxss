.top {
  width: 100%;
  height: 40px;
  line-height: 40px;
  position: relative;
  margin-bottom: 10px;
}
.pics{
  margin-top: 10px;
  height: 112px;
  overflow: hidden;
  margin-left:10px;
}
.picli{
  float: left;
  width: 100px;
  height: 100px;
  margin-right: 15px;
}
.top>.back {
  width: 30px;
  height: 30px;
  position: absolute;
  top: 8px;
  left: 20px;
}

.top>.title {
  text-align: center;
  font-size: 22px;
  font-weight: 600;
}

.pic {
  width: 95%;
  margin: 0 auto;
}

.pic>image {
  height: 110px;
  width: 100%;
  background-size: 100% 110px;
}

.box {
  width: 95%;
  margin: 0 auto;
  background: #fff;
  box-shadow: 0px 1px 5px 1px rgba(178, 178, 178, 0.08);
  border-radius: 6px;
  padding: 0 15px 0 15px;
}

.box_top {
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid #eee;
  height: 50px;
  line-height: 50px;
  padding-right: 8px;
}

.box_top_l {
  font-weight: 600;
}

.box_top_l>text {
  color: #fa789e;
  margin-right: 10px;
}

.box_top_r {
  display: flex;
}

.box_top_r>image {
  width: 30px;
  height: 30px;
  margin-top: 10px;
}

.box_top_r>view {
  font-size: 14px;
  color: #999;
}

.box_bot {
  padding: 10px 0;
}

.box_bot_item {
  display: flex;
  font-size: 14px;
  line-height: 25px;
}

.box_bot_item>view:first-child {
  color: #999;
  width: 80px;
}

.bot {
  overflow: hidden;
  position: relative;
  height: 5px;
}

.tu {
  display: flex;
  position: absolute;
  left: -20px;
  top: 0;
}

.tu_item {
  width: 45px;
  height: 5px;
  transform: skew(-25deg);
  margin: 0 5px;
}

.blue {
  background-color: skyblue;
}

.red {
  background-color: red;
}

.task {
  width: 95%;
  background: #fff;
  border-radius: 6px;
  margin: 0 auto;
}

.task_top {
  border-bottom: 1px solid #eee;
  padding: 10px 0 10px 15px;
  font-weight: 600;
  font-size: 14px;
}

.task_bot {
  border-bottom: 1px solid #eee;
  padding: 10px 0 10px 15px;  font-size: 12px;
}

.task_bot_item {
  display: flex;
  font-size: 14px;
}

.task_bot_item>view:first-child {
  color: #999;
  width: 75px;
  height: 25px;
}

.kong {
  width: 100%;
  height: 8px;
  background-color: #f0f0f0;
}

.company {
  width: 95%;
  background: #fff;
  margin: 0 auto;
  border-radius: 6px;
  font-size: 14px;
}

.company_info {
  font-weight: 600;
  height: 45px;
  line-height: 45px;
  border-bottom: 1px solid #e2e2e2;
  padding-left: 20px;
}

.company_bot {
  display: flex;
  padding: 10px 0;
  padding-left: 20px;
  position: relative;
}

.company_bot .image {
  width: 60px;
  height: 60px;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
}

.l {
  width: 60px;
  height: 60px;
  background: rgba(204, 204, 204, 1);
  border-radius: 10px;
}
.t {
  height: 60px;
  display: flex;
  width: 85px;
  justify-content: center;
  flex-direction: column;
}
.c, .r {
  width: 70px;
  height: 60px;
  display: flex;
  justify-content: center;
  flex-direction: column;
}
.r{
  margin-left: 25px;height: 70px;
}
.c {
  margin-left: 15px;
}

.c>view:first-child, .r>view:first-child, .t>view:first-child {
  margin-bottom: 10px;
}

button {
  margin-left: 15px;
  background-color: #5587ea;
  height: 60px;
  line-height: 60px;
  border-radius: 0;
  color: #fff;
  font-size: 16px;
}
.wx-auto-box{
  height: 30px;
  width: 80%;
  margin: 0 auto; 
  color: #fff;
   background: #5587ea;
  line-height: 60px;
  border-radius: 60px;
}

.nobody{
  text-align: center;
  height: 40px;
  line-height: 40px; 
}